		<div class="wrap-menu-header gradient1 trans-0-4">
			<div class="container h-full">
				<div class="wrap_header trans-0-3">
					<!-- Logo -->
					<div class="logo">
						<a asp-area="Customer" asp-controller="Home" asp-action="Index">
							<img src="/Templates/images/logo.png" alt="IMG-LOGO" data-logofixed="/Templates/images/logo.png">
						</a>
					</div>

					<!-- Menu -->
					<div class="wrap_menu p-l-45 p-l-0-xl">
						<nav class="menu">
							<ul class="main_menu">
								@if (User.Identity.IsAuthenticated)
								{
									<li>
										<a asp-area="Customer" asp-controller="Home" asp-action="Index">ANASAYFA</a>
									</li>
									<li>
										<a asp-area="Admin" asp-controller="Reservation" asp-action="Index" target="_blank">ADMİN PANELİ</a>
									</li>
									<li>
										<form class="form-inline" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home", new { area = "" })">
											<button type="submit" class="nav-link btn btn-link text-white">ÇIKIŞ</button>
										</form>
									</li>
								}
								else
								{
									<li>
										<a asp-area="Customer" asp-controller="Home" asp-action="Index">ANASAYFA</a>
									</li>
									<li>
										<a asp-area="Customer" asp-controller="Home" asp-action="Reservation">REZERVASYON</a>
									</li>
									<li>
										<a asp-area="Customer" asp-controller="Home" asp-action="Menu">MENÜ</a>
									</li>
									<li>
										<a asp-area="Customer" asp-controller="Home" asp-action="Gallery">GALERİ</a>
									</li>

									<li>
										<a asp-area="Customer" asp-controller="Home" asp-action="About">HAKKIMIZDA</a>
									</li>

									<li>
										<a asp-area="Customer" asp-controller="Home" asp-action="Contact">İLETİŞİM</a>
									</li>
								}
							</ul>
						</nav>
					</div>

					<!-- Social -->
					<div class="social flex-w flex-l-m p-r-20">
						<a href="https://www.facebook.com/kocaelisaglik.t.universitesi" target="_blank" class="fs-15 c-white"><i class="fa fa-facebook m-l-18" aria-hidden="true"></i></a>
